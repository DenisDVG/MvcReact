@using React.AspNet

@model List<MvcReact.ViewModels.NoteViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="text-center">
    <h1 class="display-4">Note Index</h1>
    <a href='@Url.Action("Create", "Note")'>Note Create</a>
    <a href='@Url.Action("Index", "Note")'>Note Index</a>
</div>


<h2>Notes List</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var note in Model)
        {
            <tr>
                <td>@note.Id</td>
                <td>@note.Title</td>
                <td>@note.Content</td>
            </tr>
        }
    </tbody>
</table>

@Html.React("Note", new{
		initialData = Model,
	});
<script src="@Url.Content("~/js/Note.jsx")"></script>